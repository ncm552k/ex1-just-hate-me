{
  "name": "file-structure-v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.0",
    "yup": "^1.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js --ext .ts --ext .tsx --ext .jsx",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:js": "prettier --write \"src/**/*.js\"",
    "format:json": "prettier --write \"src/**/*.json\"",
    "format:ts": "prettier --write \"src/**/*.ts{,x}\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:check": "prettier --check .",
    "format:check:js": "prettier --check \"src/**/*.js\"",
    "format:check:json": "prettier --check \"src/**/*.json\"",
    "format:check:ts": "prettier --check \"src/**/*.ts{,x}\"",
    "format:check:md": "prettier --check \"**/*.md\"",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn format",
      "yarn lint",
      "git add ."
    ],
    "*.{json}": [
      "yarn format",
      "git add ."
    ]
  }
}
